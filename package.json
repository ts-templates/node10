{
  "name": "@ts-templates/node10",
  "version": "0.1.0",
  "description": "Template for TypeScript project using Node.js v10",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint '**/*.{js,ts}'",
    "prebuild": "npm run clean",
    "prepare": "husky install",
    "start": "ts-node src/main.ts",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": "10.17.0"
  },
  "devDependencies": {
    "@jest/types": "27.4.2",
    "@tsconfig/node10": "1.0.8",
    "@types/jest": "27.4.0",
    "@types/node": "10.17.60",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "husky": "6.0.0",
    "jest": "27.4.7",
    "lint-staged": "11.1.4",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "ts-node": "9.1.1",
    "typescript": "4.6.3"
  },
  "license": "MIT"
}
