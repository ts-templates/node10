{
  "extends": [
    "github>ts-templates/renovate-config"
  ],
  "force": {
    "constraints": {
      "node": "10.17.0"
    }
  },
  "labels": ["renovate"],
  "packageRules": [
    {
      "matchPackageNames": ["@jest/types"],
      "allowedVersions": "<28"
    },
    {
      "matchPackageNames": ["@types/jest"],
      "allowedVersions": "<28"
    },
    {
      "matchPackageNames": ["@types/node"],
      "allowedVersions": "<11"
    },
    {
      "matchPackageNames": ["@typescript-eslint/eslint-plugin"],
      "allowedVersions": "<5"
    },
    {
      "matchPackageNames": ["eslint"],
      "allowedVersions": "<8"
    },
    {
      "matchPackageNames": ["eslint-plugin-jest"],
      "allowedVersions": "<25"
    },
    {
      "matchPackageNames": ["eslint-plugin-promise"],
      "allowedVersions": "<6"
    },
    {
      "matchPackageNames": ["husky"],
      "allowedVersions": "<7"
    },
    {
      "matchPackageNames": ["jest"],
      "allowedVersions": "<28"
    },
    {
      "matchPackageNames": ["lint-staged"],
      "allowedVersions": "<11.2"
    },
    {
      "matchPackageNames": ["node"],
      "allowedVersions": "<11"
    },
    {
      "matchPackageNames": ["ts-jest"],
      "allowedVersions": "<28"
    },
    {
      "matchPackageNames": ["ts-node"],
      "allowedVersions": "<10"
    }
  ]
}
