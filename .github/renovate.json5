{
  "extends": [
    "config:base"
  ],
  "force": {
    "constraints": {
      "node": "10.17.0",
      "npm": "6.14.15"
    }
  },
  "packageRules": [
    {
      // lint-staged v11.2 dropped support for Node.js v10
      // https://github.com/okonet/lint-staged/releases/tag/v11.2.0
      "matchPackageNames": ["lint-staged"],
      "allowedVersions": "<11.2"
    },
    {
      // ts-node v10 dropped support for Node.js v10
      // https://github.com/TypeStrong/ts-node/releases/tag/v10.0.0
      "matchPackageNames": ["ts-node"],
      "allowedVersions": "<10"
    }
  ]
}
